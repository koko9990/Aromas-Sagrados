            <img src="/images/ajax-loader.gif" alt=""/>
        </div>
    </div>
    <nav class="navbar navbar-expand-md navbar-dark bg-info navbar-inverse">
    <a class="navbar-brand" href="/">Aromas Sagrados</a>
        <div id="navbar" class="container-fluid">
          <ul class="nav navbar-nav">
              <% pages.forEach(function(page){ %>
              <% if (page.slug == "home") { %>
              <li class="nav-item"><a class="nav-link" href="/"><i class="fas fa-home"> </i> <%= page.title %> </a></li>
              <li class="nav-item"><a class="nav-link" href="/#ubicanos"><i class="fas fa-map-marked-alt"></i> Ubicanos </a></li>
              <li class="nav-item"><a class="nav-link" href="/#contactenos"><i class="fas fa-phone-volume"></i> Contactenos </a></li>
              <li class="nav-item"><a class="nav-link" href="/#testimonials2"><i class="fas fa-newspaper"></i> Noticias </a></li>
              <li class="nav-item"><a class="nav-link" href="https://www.facebook.com/aromassagradoss/"><i class="fab fa-facebook-square"></i></a></li>
              <% } else {%>
              <li class="nav-item"><a class="nav-link" href="/<%= page.slug %>"><%= page.title %> </a></li>
              <% } %>
              <% }); %>
          </ul>
          <ul class="nav navbar-nav navbar-right">
            <li>
                <a class="nav-link" href="/cart/checkout">Mi carrito <i class="fas fa-shopping-cart"></i> (
                    <% if (typeof cart !== "undefined") { %>
                    <%= cart.length %>
                    <% } else {%>
                    0
                    <% } %>
                )</a>
            </li>
            <% if (user) { %>
            <li><a class="nav-link" href="/users/logout">Hi, <i class="fas fa-user-shield"></i> <%= user.username %> ( logout )</a></li>
            <% } else {%>
            <li><a class="nav-link" href="/users/register"><i class="fas fa-user"></i> Registrate</a></li>
            <li><a class="nav-link" href="/users/login"><span class="glyphicon glyphicon-log-in"></span><i class="fas fa-sign-out-alt"></i> Log in</a></li>
            <% } %>
            <% if (user && user.admin == 1) { %>
            <li><a class="nav-link" href="/admin/pages">Admin Area</a></li>
            <% }%>
          </ul>
    </div>

</nav>

